<ul>
    <li class="@(Model.Second.Count==0?"active":"")">  
        <a href="/"><i class="zmdi zmdi-home"></i>Home</a>
    </li>
    @foreach (var o in Model.First)
    {
        var children = Model.First.Where(x => x.Pid == o.Id);
        if (children != null)
        {
            <li class="sub-menu">
                <a href="@(o.Link)" data-ma-action="submenu-toggle"><i class="zmdi zmdi-chart"></i>@(o.Name)</a>

                <ul>
                    @foreach (var c in children)
                    {
                        <li class="@(Model.Second.Contains(c.Id)?"active":"")">
                            <a href="@(c.Link)"><i class="zmdi zmdi-home"></i>@(c.Name)</a>
                        </li>
                    }

                </ul>
            </li>
        }
        else
        {
            <li class="@(Model.Second.Contains(o.Id)?"active":"")">
                <a href="@(o.Link)"><i class="zmdi zmdi-home"></i>@(o.Name)</a>
            </li>
        }
    }
</ul>